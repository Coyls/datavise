  "bugs": {
    "url": "https://github.com/Coyls/datavise/issues"
  },
  "homepage": "https://github.com/Coyls/datavise#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "neo4j-driver": "^4.3.3"
  },
  "devDependencies": {
    "csvtojson": "^2.0.10",
    "cz-conventional-changelog": "^3.3.0"
  },
