{
  "name": "datavise",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config index.js",
    "json-country": "node -r dotenv/config ./database/scripts/country.js",
    "csv-country": "json2csv -i ./database/data-json/country.json -f name,noc -o ./database/data-csv/country.csv",
    "json-athlete": "node -r dotenv/config ./database/scripts/athlete",
    "csv-athlete": "json2csv -i ./database/data-json/athlete.json -f name,sex,height,wheight -o ./database/data-csv/athlete.csv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coyls/datavise.git"
  },
  "keywords": [],
  "author": "",
