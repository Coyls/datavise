{
  "name": "datavise",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config index.js",
    "rm-data": "rm ./database/data-json/* && rm ./database/data-csv/*",
    "country": "node -r dotenv/config ./database/scripts/country.js && json2csv -i ./database/data-json/country.json -f noc,name -o ./database/data-csv/country.csv",
    "athlete": "node -r dotenv/config ./database/scripts/athlete.js && json2csv -i ./database/data-json/athlete.json -f id,name,sex,height,wheight,team,noc -o ./database/data-csv/athlete.csv",
    "sport": "node -r dotenv/config ./database/scripts/sport.js && json2csv -i ./database/data-json/sport.json -f id,name -o ./database/data-csv/sport.csv",
    "year": "node -r dotenv/config ./database/scripts/year.js && json2csv -i ./database/data-json/year.json -f year -o ./database/data-csv/year.csv",
    "season": "node -r dotenv/config ./database/scripts/season.js && json2csv -i ./database/data-json/season.json -f season -o ./database/data-csv/season.csv",
    "event": "node -r dotenv/config ./database/scripts/event.js && json2csv -i ./database/data-json/event.json -f id,name,id_sport -o ./database/data-csv/event.csv",
    "age": "node -r dotenv/config ./database/scripts/age.js && json2csv -i ./database/data-json/age.json -f id,age,year,id_athlete -o ./database/data-csv/age.csv",
    "city": "json2csv -i ./database/raw-data/city.json -f id,noc,name,lat,lng -o ./database/data-csv/city.csv && cp ./database/raw-data/city.json ./database/data-json/city.json",
    "gpd": "node -r dotenv/config ./database/scripts/gpd.js && json2csv -i ./database/data-json/gpd.json -f id,noc,year,value -o ./database/data-csv/gpd.csv",
    "jo": "node -r dotenv/config ./database/scripts/jo.js && json2csv -i ./database/data-json/jo.json -f id,year,id_city,season -o ./database/data-csv/jo.csv",
    "medal": "node -r dotenv/config ./database/scripts/medal.js && json2csv -i ./database/data-json/medal.json -f id,type,id_jo,id_event,id_athlete -o ./database/data-csv/medal.csv",
    "generate-csv": "npm run country && npm run year && npm run sport && npm run event && npm run season && npm run gpd && npm run city && npm run jo && npm run athlete && npm run age && npm run medal"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coyls/datavise.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Coyls/datavise/issues"
  },
  "homepage": "https://github.com/Coyls/datavise#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "neo4j-driver": "^4.3.3"
  },
  "devDependencies": {
    "cities.json": "^1.1.5",
    "csvtojson": "^2.0.10",
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
